{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "jsx": "react-native",
    "baseUrl": "./src",
    "paths": {
      "@components/*": [
        "components/*"
      ],
      "@assets/*": [
        "assets/*"
      ],
      "@utils/*": [
        "utils/*"
      ],
      "@constants/*": [
        "constants/*"
      ],
      "@navigation/*": [
        "navigation/*"
      ],
      "@screens/*": [
        "screens/*"
      ],
      "@hooks/*": [
        "hooks/*"
      ],
      "@types/*": [
        "types/*"
      ],
      "@services/*": [
        "services/*"
      ],
      "@store/*": [
        "store/*"
      ],
      "@config/*": [
        "config/*"
      ],
      "@i18n/*": [
        "i18n/*"
      ],
      "@theme/*": [
        "theme/*"
      ],
      "@hoc/*": [
        "hoc/*"
      ]
    },
    "noEmit": true
  },
}